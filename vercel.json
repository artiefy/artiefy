{
  "version": 2,
  "functions": {
    "app/api/courses/**/*.ts": {
      "maxDuration": 60
    },
    "app/estudiantes/**/*": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/courses",
      "dest": "/api/courses"
    }
  ],
  "crons": [
    {
      "path": "/api/revalidate",
      "schedule": "0 0 * * *"
    }
  ]
}

