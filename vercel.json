{
  "functions": {
    "app/api/upload": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "installCommand": "npm install",
  "public": true,
  "regions": ["sfo1", "iad1"],
  "functionFailoverRegions": ["iad1", "sfo1"],
  "crons": [
    {
      "path": "/api/cron/check-subscriptions",
      "schedule": "*/5 * * * *"
    }
  ],
  "trailingSlash": false
}
