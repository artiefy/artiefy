{
  "functions": {
    "app/api/upload/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "framework": "nextjs",
  "headers": [
    {
      "source": "/app/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "installCommand": "npm install",
  "public": true
}
